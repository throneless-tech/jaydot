{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@payload-config": [
        "./src/app/payload.config.ts"
      ],
      "@components/*": [
        "src/app/(frontend)/components/*"
      ],
      "@layouts/*": [
        "src/app/(frontend)/layouts/*"
      ],
      "@assets/*": [
        "src/app/(frontend)/assets/*"
      ],
      "@/utils/*": [
        "src/app/(frontend)/utils/*"
      ]
    }
  }
}